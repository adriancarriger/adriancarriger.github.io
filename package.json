{
  "name": "using-typescript",
  "version": "1.0.0",
  "description": "Gatsby example site demonstrating how to use Typescript with Gatsby",
  "author": "fabien0102 <fabien0102@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "deploy": "gh-pages -d public",
    "semantic-release": "semantic-release",
    "semantic-commitlint": "semantic-commitlint"
  },
  "dependencies": {
    "gatsby": "latest",
    "gatsby-link": "latest",
    "gatsby-plugin-typescript": "latest"
  },
  "devDependencies": {
    "@semantic-release/exec": "^2.2.3",
    "@types/node": "^8.0.7",
    "@types/react": "15.0.21",
    "@types/react-dom": "0.14.23",
    "condition-circle": "^2.0.1",
    "gh-pages": "^1.1.0",
    "semantic-commitlint": "^1.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adriancarriger/adriancarriger.github.io.git"
  },
  "release": {
    "branch": "master",
    "verifyConditions": [
      "@semantic-release/git",
      "@semantic-release/github",
      "condition-circle"
    ],
    "getLastRelease": "@semantic-release/git",
    "publish": [
      "@semantic-release/github",
      {
        "path": "@semantic-release/exec",
        "cmd": "yarn run deploy"
      }
    ],
    "verifyRelease": [
      "semantic-commitlint"
    ]
  }
}
